APP_ENV=local
APP_DEBUG=true
APP_KEY=
APP_VERSION=v1.11.1
PERF_BUDGET_TOTAL_MS_WARN=400
PERF_BUDGET_TOTAL_MS_HARD=1200
PERF_BUDGET_SQL_COUNT_WARN=40
PERF_BUDGET_SQL_COUNT_HARD=120
PERF_BUDGET_SQL_MS_WARN=150
PERF_BUDGET_SQL_MS_HARD=600
PERF_BUDGET_ENABLED=true
PERF_BUDGET_HARD_FAIL=false
DB_PROFILE_STORE_SQL=false
CACHE_TTL_DAYS=7
ASSET_BASE=/assets
ASSET_VENDOR_BASE=/assets/vendor
ASSET_APP_BASE=/assets/app
ASSET_BOOTSTRAP_VERSION=5.3.3
ASSET_HTMX_VERSION=1.9.12
ASSET_BOOTSTRAP_ICONS_VERSION=1.11.3
APP_READ_ONLY=false
POLICY_STRICT=false
APP_ENFORCE_UI_TOKENS=false
CSP_MODE=enforce
APP_HEADLESS=false
APP_HEADLESS_HTML_ALLOWLIST=/login,/logout,/admin
APP_HEADLESS_HTML_OVERRIDE_PARAM=_html
# Pages content normalization (default off)
APP_PAGES_NORMALIZE_ENABLED=0
# Menu content normalization (default off)
APP_MENU_NORMALIZE_ENABLED=0
# DevTools JS errors normalization (default off)
APP_DEVTOOLS_JS_NORMALIZE_ENABLED=0
# Security report content normalization (default off)
SECURITY_REPORTS_NORMALIZE_ENABLED=0
HEALTH_WRITE_CHECK=false
# Bootstrap pipeline (default off)
# APP_BOOTSTRAPS_ENABLED=1 enables the bootstrap runner.
# APP_BOOTSTRAPS_MODULES_TAKEOVER=1 moves module bootstrapping into ModulesBootstrap.
# APP_BOOTSTRAPS="security,observability,modules,routing,view" limits bootstraps to listed entries.
# APP_ROUTING_CACHE_WARM=1 warms route cache during bootstrap; APP_ROUTING_CACHE_WARM_FORCE=1 forces rebuild.
# APP_VIEW_SANITY_STRICT=1 enables strict view sanity checks (dev only).
APP_BOOTSTRAPS_ENABLED=0
APP_BOOTSTRAPS_MODULES_TAKEOVER=0
APP_ROUTING_CACHE_WARM=0
APP_ROUTING_CACHE_WARM_FORCE=0
APP_VIEW_SANITY_STRICT=0
DB_DRIVER=mysql
DB_HOST=localhost
DB_PORT=3306
DB_NAME=laas
DB_USER=root
DB_PASSWORD=
ADMIN_SEED_ENABLED=true
ADMIN_SEED_PASSWORD=change-me
SESSION_TIMEOUT=7200
SESSION_IDLE_TTL=0
SESSION_ABSOLUTE_TTL=0
SESSION_COOKIE_SECURE=false
SESSION_COOKIE_SAMESITE=Lax
SESSION_COOKIE_DOMAIN=
SESSION_DRIVER=native
REDIS_URL=redis://127.0.0.1:6379/0
REDIS_TIMEOUT=1.5
REDIS_PREFIX=laas:sess:
MEDIA_MAX_BYTES=10485760
MEDIA_PUBLIC=false
MEDIA_PUBLIC_MODE=private
MEDIA_SIGNED_URLS_ENABLED=true
MEDIA_SIGNED_URL_TTL_SECONDS=600
MEDIA_SIGNED_URL_SECRET=change_me
MEDIA_ALLOWED_MIME=image/jpeg,image/png,image/webp,application/pdf
STORAGE_DISK=local
S3_ENDPOINT=
S3_REGION=
S3_BUCKET=
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_USE_PATH_STYLE=false
S3_PREFIX=
S3_TIMEOUT_SECONDS=10
S3_VERIFY_TLS=true

API_CORS_ENABLED=false
API_CORS_ORIGINS=
API_CORS_METHODS=GET,POST,PUT,PATCH,DELETE,OPTIONS
API_CORS_HEADERS=Authorization,Content-Type,X-Requested-With
API_CORS_MAX_AGE=600
API_RATE_LIMIT_PER_MINUTE=120
API_RATE_LIMIT_BURST=30
API_TOKEN_ISSUE_MODE=admin
API_TOKEN_SCOPES=admin.read,admin.write,media.read,media.write,pages.read
